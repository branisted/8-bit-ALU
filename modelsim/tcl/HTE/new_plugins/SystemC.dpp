sc_mutex=channelCmd
sc_prim_channel=channelCmd
sc_semaphore=channelCmd
sc_signal=channelCmd
sc_signal_resolved=channelCmd
sc_signal_rv=channelCmd
SC_MODULE=moduleCmd
SC_CTOR=moduleCmd

