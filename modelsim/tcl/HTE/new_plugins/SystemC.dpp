[TagNames]
builtinConstant="Builtin Constant"
builtinFunction="Builtin Function"
builtinMacros="Builtin Macro"
chanTypes="Channel Type"
channelDeclaration="Channel Name"
char="Character"
codeBlockBound="Code Block Bound"
comment="Comment"
dataType="Data Type"
integer="Integer"
interfaceTypes="Interface Type"
keyWord="Keyword"
