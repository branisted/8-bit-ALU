[General]
languageName=SystemC
extensions=cpp|h
scripts=$HTE_HOME/new_plugins/SystemC.tcl
iconsPath=$HTE_HOME/images
version=1.0

[Syntax]
ignoreCase=No
stringStart="
stringEnd="
charStart='
charEnd='
preprocMark=#
highlightGroups=keyWord builtinFunction builtinMacros builtinConstant dataType interfaceTypes chanTypes portTypes timeUnit logicValue oMode qMode

[Style]
tabWidth=4
softTabs=Yes
autoIndent=Yes

[Comment]
singleLine=//
multiLineStart=/*
multiLineEnd=*/
psl=yes

[HLCallbacks]
sc_fifo_in=portCmd
sc_fifo_out=portCmd
sc_in=portCmd
sc_in_clk=portCmd
