  parameter ZI_TLP_HEADER_STATE = 5;
  parameter ZI_TLP_DATA_STATE = 6;
  parameter ZI_TLP_DIGEST_STATE = 7;
  parameter ZI_TLP_LCRC_STATE = 8;
  parameter ZI_TLP_UNKNOWN_STATE = 9;

  parameter ZI_STP = 8'hFB; // K27.7 
  parameter ZI_END = 8'hFD; // K29.7 
  parameter ZI_EDB = 8'hFE; // K27.7

  // Parameter declarations for request types

  parameter ZI_MEM_REQ = 0; // Memory requests
  parameter ZI_MEMLK_REQ = 1; // Locked requests
  parameter ZI_IO_REQ = 2; // IO requests
  parameter ZI_MSGAS_REQ = 3; // Message with advance switching not supported in Gen2
  parameter ZI_CFG0_REQ = 4; // Configuration type 0 requests 
  parameter ZI_CFG1_REQ = 5; // Configuration type 1 requests 
  parameter ZI_CPL = 10; // Completions 
  parameter ZI_CPL_LK = 11; // Completions for locked requests

  parameter ZI_MSG_REQ = 2;

