`endprotected
  // Wire assignments

  assign fmt = (level_select) ? fmt_posedge : fmt_negedge;
  assign tlp_type = (level_select) ? type_posedge : type_negedge;
  assign tc = (level_select) ? tc_posedge : tc_negedge;
  assign attr = (level_select) ? attr_posedge : attr_negedge;
  // PCI_EXPRESS_GEN2 code start 
  assign at = (level_select) ? at_posedge : at_negedge;
  // PCI_EXPRESS_GEN2 code end
  assign td = (level_select) ? td_posedge : td_negedge;
  assign ep = (level_select) ? ep_posedge : ep_negedge;
  assign length = (level_select) ? length_posedge : length_negedge;
  assign cpl_status = (level_select) ? cpl_status_posedge : cpl_status_negedge;
  assign address_64_bit = (level_select) ?
                          address_64_bit_posedge : address_64_bit_negedge;
  assign last_dw_be = (level_select) ? 
                      last_dw_be_posedge : last_dw_be_negedge;
  assign first_dw_be = (level_select) ?                              
                        first_dw_be_posedge : first_dw_be_negedge;
  assign requester_id = (level_select) ? 
                        requester_id_posedge : requester_id_negedge;
  assign tag = (level_select) ? tag_posedge : tag_negedge;
  assign tlp_digest = (level_select) ? tlp_digest_posedge : tlp_digest_negedge;
  assign enable_ecrc = (level_select) ? 
                       enable_ecrc_posedge : enable_ecrc_negedge;
  assign ecrc = (level_select) ? ecrc_posedge : ecrc_negedge;
 
  // Additional gen1 code start
  assign unsupported_request = (level_select) ?
                               unsupported_request_posedge : unsupported_request_negedge;
  assign last_captured_req_id = (level_select) ?
      last_captured_req_id_posedge : last_captured_req_id_negedge;

  assign initial_cfg_started = (level_select) ?
      initial_cfg_started_posedge : initial_cfg_started_negedge;
  assign sspl_sent = (level_select)?
      sspl_sent_posedge : sspl_sent_negedge;
  assign lock_established = (level_select) ?
      lock_established_posedge : lock_established_negedge;
  assign lock_completed = (level_select) ?
      lock_completed_posedge : lock_completed_negedge;  
  // Additional gen1 code end 
  assign tlp_present_state = (level_select) ?
                          tlp_present_state_posedge : tlp_present_state_negedge;
  assign header_byte_count = (level_select) ?
                          header_byte_count_posedge : header_byte_count_negedge;
  assign tlp_byte_count = (level_select) ? 
                          tlp_byte_count_posedge : tlp_byte_count_negedge;
  assign lcrc_byte_count = (level_select) ?
                          lcrc_byte_count_posedge : lcrc_byte_count_negedge;
  assign tlp_digest_byte_count = (level_select) ?
                          tlp_digest_byte_count_posedge :
                          tlp_digest_byte_count_negedge;

  assign completer_id = (level_select) ?
                        completer_id_posedge : completer_id_negedge;

  assign cpl_byte_count = (level_select) ?
                        cpl_byte_count_posedge : cpl_byte_count_negedge;
  assign cpl_bcm = (level_select) ?
                   cpl_bcm_posedge : cpl_bcm_negedge;

  assign message_code = (level_select) ? 
                   message_code_posedge : message_code_negedge;
  
  assign cpl_lwr_address = (level_select) ?
                   cpl_lwr_address_posedge : cpl_lwr_address_negedge;

  assign malformed_tlp = (level_select) ?
                   malformed_tlp_posedge : malformed_tlp_negedge;

  wire [31:0] int_valid_code = valid_code;

  assign valid_across_link_width = 
                ((link_width === 6'd1 && int_valid_code[0] === 1'b1) ||
                (link_width === 6'd2 && int_valid_code[1:0] === 2'b11) ||
                (link_width === 6'd4 && int_valid_code[3:0] === 4'hF) ||
                (link_width === 6'd8 && int_valid_code[7:0] === 8'hFF) ||
                (link_width === 6'd12 && int_valid_code[11:0] === 12'hFFF) ||
                (link_width === 6'd16 && int_valid_code[15:0] === 16'hFFFF) ||
                (link_width === 6'd32 && (&int_valid_code === 1'b1))); 

  wire collect_stats = 1'b1;
  wire [31:0] polynomial = ZI_ECRC_POLYNOMIAL;

  // max_payload_size_decoder

  assign maximum_payload_size[6:0] = 7'b0;
  assign maximum_payload_size[7] = (max_payload_size === 3'b000) ? 1'b1 : 1'b0;
  assign maximum_payload_size[8] = (max_payload_size === 3'b001) ? 1'b1 : 1'b0;
  assign maximum_payload_size[9] = (max_payload_size === 3'b010) ? 1'b1 : 1'b0;
  assign maximum_payload_size[10] = (max_payload_size === 3'b011) ? 1'b1 : 1'b0;
  assign maximum_payload_size[11] = (max_payload_size === 3'b100) ? 1'b1 : 1'b0;
  assign maximum_payload_size[12] = (max_payload_size === 3'b101) ? 1'b1 : 1'b0;

  // max_read_req_size_decoder

  assign maximum_read_req_size[6:0] = 7'b0;
  assign maximum_read_req_size[7] = (max_read_req_size === 3'b000) ? 1'b1 : 1'b0;
  assign maximum_read_req_size[8] = (max_read_req_size === 3'b001) ? 1'b1 : 1'b0;
  assign maximum_read_req_size[9] = (max_read_req_size === 3'b010) ? 1'b1 : 1'b0;
  assign maximum_read_req_size[10] = (max_read_req_size === 3'b011) ? 1'b1 : 1'b0;
  assign maximum_read_req_size[11] = (max_read_req_size === 3'b100) ? 1'b1 : 1'b0;
  assign maximum_read_req_size[12] = (max_read_req_size === 3'b101) ? 1'b1 : 1'b0;

