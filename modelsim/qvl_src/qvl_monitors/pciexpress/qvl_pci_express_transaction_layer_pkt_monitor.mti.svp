  reg sspl_sent_posedge; 
  reg sspl_sent_negedge;
  reg next_sspl_sent;
  reg lock_established_posedge; 
  reg lock_established_negedge;
  reg next_lock_established;
  reg lock_completed_posedge; 
  reg lock_completed_negedge;
  reg next_lock_completed;
  // Additional gen1 code end

  reg [3:0] temp_tlp_current_state;
